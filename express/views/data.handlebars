<h1>Hi- {{name}}</h1>
<!--Render in form table-->


<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Age</th>
      <th scope="col">Name</th>
      <th scope="col">Address</th>
    </tr>
  </thead>
  <tbody>
      {{#each list}}
    <tr>
      <th scope="row">{{this._id}}</th>
      <td>{{this.age}}</td>
      <td>{{this.name}}</td>
      <td>{{this.address}}</td>
    </tr>
  {{/each}}
  
   

  </tbody>
</table>
